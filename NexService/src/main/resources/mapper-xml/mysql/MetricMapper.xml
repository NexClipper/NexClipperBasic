<mapper namespace="com.nexcloud.rdb.mapper.mysql.MetricRepository">
	<select id="getMetricList" resultType="com.nexcloud.rdb.domain.mysql.Metric">
		SELECT
			idx,
			target_system targetSystem,
			target,
			data_source dataSource,
			`table`,
			metric,
			alias,
			math,
			group_by groupBy,
			new_engine newEngine,
			regdt
		FROM
			nexclipper_metric
	</select>
</mapper>
